{
  "name": "servable-engine",
  "type": "module",
  "source": "src/index.js",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "version": "0.0.0-development",
  "description": "Servable engine for Parse.",
  "author": "Aboubacar Doucoure",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yelounak/servable-engine/issues"
  },
  "homepage": "https://github.com/yelounak/servable-engine#readme",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "access": "public"
  },
  "release": {
    "branches": [
      "main"
    ]
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "dev": "microbundle watch",
    "test_": "echo \"Error: no test specified\" && exit 1",
    "semantic-release": "semantic-release",
    "acp": "git add . && npm run commit && git push",
    "commit": "git-cz",
    "size": "size-limit",
    "analyze": "size-limit --why",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yelounak/servable-engine.git"
  },
  "keywords": [
    "parse",
    "server"
  ],
  "prettier": {
    "printWidth": 80,
    "semi": true,
    "singleQuote": true,
    "trailingComma": "es5"
  },
  "size-limit": [
    {
      "path": "dist/index.cjs.production.min.js",
      "limit": "10 KB"
    },
    {
      "path": "dist/index.esm.js",
      "limit": "10 KB"
    }
  ],
  "dependencies": {
    "agenda": "*",
    "axios": "^0.27.2",
    "bluebird": "^3.7.2",
    "docker-compose": "0.24.1",
    "express": "^4.18.1",
    "get-port": "^6.1.2",
    "inspirational-quotes": "^2.0.1",
    "json-diff": "^1.0.6",
    "keynum": "*",
    "memory-cache": "^0.2.0",
    "mime-types": "^2.1.35",
    "minio": "^7.0.28",
    "moment": "*",
    "mongodb": "^4.13.0",
    "mongodb-snapshot": "^1.4.1",
    "mongodb-uri": "^0.9.7",
    "mongoose": "^6.8.1",
    "mydockerjs": "^2.14.2",
    "parse": "^3.5.1",
    "parse-server": "^5.4.0",
    "parse-server-s3like-adapter": "^2.0.1",
    "parse-server-sendgrid-adapter": "^1.0.0",
    "query-string-parse": "*",
    "semver": "*",
    "underscore": "^1.13.3",
    "url-parse": "*",
    "winston": "^3.7.2",
    "yaml": "^2.3.0",
    "js-sha256": "^0.9.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-unicode-property-regex": "^7.18.6",
    "@babel/plugin-syntax-import-assertions": "^7.22.5",
    "@babel/plugin-syntax-import-attributes": "^7.22.5",
    "@babel/preset-env": "^7.22.15",
    "@size-limit/preset-small-lib": "^4.10.2",
    "babel-eslint": "^10.1.0",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^6.0.0",
    "javascript-obfuscator": "^4.0.0",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "semantic-release": "^21.1.1",
    "size-limit": "^4.10.2",
    "rollup-plugin-import-assertions": "^0.5.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-standard": "^14.1.0",
    "eslint-config-standard-react": "^9.2.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-node": "^11.0.0",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-standard": "^4.0.1",
    "prettier": "^2.0.4"
  }
}
